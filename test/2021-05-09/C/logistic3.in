`AP1 [ I, A, B ] {
	assumptions:
	conclusions:
		(I->A)->(I->(B->A))
}

`AP2 [ I, A, B, C ] {
	assumptions:
	conclusions:
		(I->(A->(B->C)))->(I->((A->B)->(A->C)))
}

`AP3 [ I, A, B ] {
	assumptions:
	conclusions:
		(I->(!A->!B))->(I->(B->A))
}

`AMP [ I, A, B ] {
	assumptions:
		I->A
		I->(A->B)
	conclusions:
		I->B
}
